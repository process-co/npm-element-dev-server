#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_bde3cb843fdd2c351d39b748fa4b53df/node_modules/ts-jest/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_bde3cb843fdd2c351d39b748fa4b53df/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_bde3cb843fdd2c351d39b748fa4b53df/node_modules/ts-jest/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.10_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest_bde3cb843fdd2c351d39b748fa4b53df/node_modules:/home/runner/work/proc-app/proc-app/out/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
